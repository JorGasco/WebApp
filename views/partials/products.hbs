<table class="ui fixed striped table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Category</th>
 <th>image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{description}}
        </td>
        <td>
          ${{price}}
        </td>
        <td>
          {{category}}
        </td>
         <td>
          {{image}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>