<div class="ui card" id ="{{name}}">
  <div class="image">
    <img src="{{image}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date">Joined in {{joined}}</span>
    </div>
    <div class="description">
      {{name}} is {{job}} in BestTicket App
  </div>
  <div class="extra content">
    <a>
      <i class="point icon"></i>
      {{city}} 
    </a>
  </div>
</div>
